{
  "source": "./src/client/app",
  "destination": "docs",
  "includes": ["\\.(js|es6)$"],
  "excludes": ["\\.test\\.(js|es6)$"],
  "access": ["public", "protected"],
  "autoPrivate": true,
  "unexportIdentifier": true,
  "undocumentIdentifier": false,
  "builtinExternal": true,
  "index": "./README.md",
  "package": "./package.json",
  "coverage": true,
  "includeSource": true,
  "title": "React Template Builder",
  "styles": ["src/client/scss"],
  "test": {
    "type": "mocha",
    "source": "./src/client/app",
    "includes": ["\\.test\\.(js|es6)$"]
  },
  "manual": {
    "installation": ["./README.md"]
  },
  "lint": true
}
